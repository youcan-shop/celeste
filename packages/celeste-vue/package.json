{
  "name": "@youcan/celeste-vue",
  "type": "module",
  "version": "0.0.0",
  "author": "YouCan <tech@youcan.shop>",
  "keywords": [
    "youcan",
    "celeste"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && pnpm build-only",
    "build-only": "vite build",
    "watch": "vite build --watch",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "type-gen": "vue-tsc --declaration  --emitDeclarationOnly",
    "test": "vitest"
  },
  "dependencies": {
    "@youcan/celeste-icons": "workspace:*",
    "@youcan/celeste-tokens": "workspace:*",
    "clsx": "^2.1.1",
    "radix-vue": "^1.9.12"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^25.0.1",
    "unocss": "^0.65.3",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  }
}
