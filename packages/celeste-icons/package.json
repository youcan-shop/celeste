{
  "name": "@youcan/celeste-icons",
  "type": "module",
  "version": "0.1.3",
  "author": "YouCan <tech@youcan.shop>",
  "keywords": [
    "youcan",
    "celeste"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./icons.json": {
      "default": "./dist/icons.json"
    },
    "./info.json": {
      "default": "./dist/info.json"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/esm/index.js",
      "require": "./dist/vue/index.js"
    },
    "./vue/*": {
      "types": "./dist/vue/*.d.ts",
      "import": "./dist/vue/esm/*.js",
      "require": "./dist/vue/*.js"
    },
    "./vue/*.js": {
      "types": "./dist/vue/*.d.ts",
      "import": "./dist/vue/esm/*.js",
      "require": "./dist/vue/*.js"
    },
    "./vue/esm/*": {
      "types": "./dist/vue/*.d.ts",
      "import": "./dist/vue/esm/*.js"
    },
    "./vue/esm/*.js": {
      "types": "./dist/vue/*.d.ts",
      "import": "./dist/vue/esm/*.js"
    }
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "generate-iconify": "tsx scripts/generate-iconify",
    "generate-vue": "tsx scripts/generate-vue.ts",
    "build": "rimraf dist && pnpm generate-iconify && pnpm generate-vue && tsup",
    "release": "pnpm publish --access public"
  },
  "dependencies": {
    "@iconify/types": "^2.0.0"
  },
  "devDependencies": {
    "@iconify/tools": "^4.1.1",
    "@iconify/utils": "^2.2.1",
    "@svgr/core": "^8.1.0",
    "@types/fs-extra": "^11.0.4",
    "@vue/compiler-dom": "^3.5.13",
    "camelcase": "^8.0.0",
    "rimraf": "^6.0.1",
    "svgo": "^3.3.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2"
  }
}
